{
  "version": 2,
  "functions": {
    "back/api/index.ts": {
      "runtime": "@vercel/node@18"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/back/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/front/dist/index.html"
    }
  ],
  "buildCommand": "cd front && yarn build"
}